    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>CampaignAttributesRequired</fullName>
        <active>true</active>
        <description>This validation rule is required to enforce the picklist field requiredness on the custom VF page(grid).</description>
        <errorConditionFormula>ISBLANK(Name) ||
ISBLANK(TEXT(CampaignType__c)) || 
ISBLANK(TEXT(GLAccountType__c)) || 
ISBLANK(TEXT(GLDescription__c)) || 
ISBLANK(TEXT(GLCode__c))</errorConditionFormula>
        <errorMessage>Campaign Name. Campaign Type. GL Account Type. GL Description. GL Code must be populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CampaignBudgetCannotbeLessThanZero</fullName>
        <active>true</active>
        <description>Campaign Budget cannot be less than zero dollars.</description>
        <errorConditionFormula>PlannedCampaignSpend__c &lt; 0</errorConditionFormula>
        <errorDisplayField>PlannedCampaignSpend__c</errorDisplayField>
        <errorMessage>Campaign Budget cannot be less than zero dollars. Please update the Campaign Budget field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CampaignCreatedInPriorYear</fullName>
        <active>false</active>
        <description>Campaigns cannot be created if the Master Marketing Plan is associated with a previous Year</description>
        <errorConditionFormula>AND( 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;System Administrator with Bulk Hard Delete&quot;, 
VALUE(( Year__c )) &lt; YEAR( TODAY() ))</errorConditionFormula>
        <errorMessage>Campaigns cannot be created if the Master Marketing Plan is associated with a previous Year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CampaignCurrencyMustEqualMP</fullName>
        <active>false</active>
        <description>Campaign Currency must equal Marketing Plan Currency.</description>
        <errorConditionFormula>text(CurrencyIsoCode)  &lt;&gt;  text(ParentMarketingPlan__r.CurrencyIsoCode)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Campaign Currency does not equal Marketing Plan Currency.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Campaign_BQ_match_MP_B</fullName>
        <active>true</active>
        <description>Campaign Brand Quality must be consistent with the Brand associated with the Parent Marketing Plan.</description>
        <errorConditionFormula>Brand_Quality__r.Brand__c &lt;&gt;  ParentMarketingPlan__r.Brand__c</errorConditionFormula>
        <errorDisplayField>Brand_Quality__c</errorDisplayField>
        <errorMessage>Brand Quality is not consistent with the Brand associated with this Marketing Plan. Please enter an appropriate Brand Quality value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Campaign_Date_Validation</fullName>
        <active>true</active>
        <description>Campaign Start Date must be less than End Date.</description>
        <errorConditionFormula>InitialStartDate__c   &gt;   InitialEndDate__c</errorConditionFormula>
        <errorDisplayField>InitialStartDate__c</errorDisplayField>
        <errorMessage>Start Date must be prior or equal to End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CampaignsOnlyCreatedwithMPCampaignRT</fullName>
        <active>true</active>
        <description>Campaigns can only be created if the Master Marketing Plan has a record type containing &apos;Campaigns Allowed&apos;.</description>
        <errorConditionFormula>AND(ParentMarketingPlan__r.RecordType.Name &lt;&gt; &quot;Campaigns Allowed - Not Editable&quot;, ParentMarketingPlan__r.RecordType.Name  &lt;&gt; &quot;Campaigns Allowed - Editable&quot;)</errorConditionFormula>
        <errorMessage>Campaigns can only be created if the Master Marketing Plan record type allows for Campaigns. Please select another Marketing Plan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_Required_for_Budget_Update</fullName>
        <active>true</active>
        <description>If the Campaign Budget is changed, the user must enter a comment as to why.</description>
        <errorConditionFormula>and(ischanged( PlannedCampaignSpend__c ),isblank( Comments__c ))</errorConditionFormula>
        <errorMessage>A Comment is required when changing a Campaign Budget value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Year_Match_Parent</fullName>
        <active>true</active>
        <description>End Date Year must match the Parent Marketing Plan.</description>
        <errorConditionFormula>text(ParentMarketingPlan__r.Year__c )  &lt;&gt;  text(YEAR( InitialEndDate__c ))</errorConditionFormula>
        <errorDisplayField>InitialEndDate__c</errorDisplayField>
        <errorMessage>End Date Year must match the Parent Marketing Plan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Forecasted_Amount_must_be_LE_Budget</fullName>
        <active>false</active>
        <errorConditionFormula>ForecastedCampaignSpend__c &gt;  PlannedCampaignSpend__c</errorConditionFormula>
        <errorMessage>You cannot Forecast more than the Campaign Budget.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IOCodeCannotBeCreatedinPriorYearPlan</fullName>
        <active>true</active>
        <description>IO Codes cannot be created for a Campaigns if the Master Marketing Plan is associated with a previous Year</description>
        <errorConditionFormula>AND( 
ISCHANGED( IOCode__c), 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;System Administrator with Bulk Hard Delete&quot;, 
VALUE(( Year__c )) &lt; YEAR( TODAY() ))</errorConditionFormula>
        <errorMessage>IO Codes cannot be created for a Campaigns if the Master Marketing Plan is associated with a previous Year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Year_Match_Parent</fullName>
        <active>true</active>
