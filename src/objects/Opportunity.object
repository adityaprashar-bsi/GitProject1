    <validationRules>
        <fullName>OpportunityNameEditing</fullName>
        <active>true</active>
        <description>Opportunity Names can only be edited by the System Admin Profile. This will ensure that opportunity names are only updated through the integration and not by any Sales 360 users without a System Admin Profile.</description>
        <errorConditionFormula>ISCHANGED(Name)&amp;&amp; 
NOT(OR(
$Profile.Name = &quot;System Administrator&quot;,
$Profile.Name = &quot;System Administrator with Bulk Hard Delete&quot;))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Opportunity Name cannot be edited as Opportunity Name is controlled by Aperity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Reason_Detail_if_Other_Selected</fullName>
        <active>true</active>
        <description>Require a value within the &quot;Reason Detail&quot; field if &quot;Other&quot; is selected as the &quot;Reason Won or Lost&quot;</description>
        <errorConditionFormula>IF( ISPICKVAL(ReasonWonorLost__c, &apos;Other&apos;), If( ISBLANK( Reason_Detail__c ), true, false), false)</errorConditionFormula>
        <errorMessage>Please explain why the opportunity was won or lost in the Reason Detail field.</errorMessage>
    </validationRules>
