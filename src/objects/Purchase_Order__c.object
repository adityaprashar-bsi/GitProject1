    <validationRules>
        <fullName>Incoterms_Changed_Requirement</fullName>
        <active>true</active>
        <description>If Incoterms were changed, Reason and Budget Code are required.</description>
        <errorConditionFormula>AND (
 ISPICKVAL( Inco_Terms_Changed__c , &quot;Yes&quot;),
 OR(
 ISBLANK( Reason__c ),
 ISBLANK( Budget_Code__c ),
 ISPICKVAL( Inco_Terms_Approved__c , &quot;&quot;) 
)
)</errorConditionFormula>
        <errorMessage>Incoterms Approved, Reason and Budget Code fields are Required because Incoterms were changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Issue_Required_on_PO_Issue</fullName>
        <active>true</active>
        <description>If Purchase Order Issue is &quot;Other&quot;, this requires &quot;Other Issue&quot; to be populated.</description>
        <errorConditionFormula>AND ( 
INCLUDES(  PurchaseOrderIssues__c , &quot;Other&quot;), 
ISBLANK(  Other_Issue__c  ) 
)</errorConditionFormula>
        <errorDisplayField>Other_Issue__c</errorDisplayField>
        <errorMessage>This field is required.</errorMessage>
    </validationRules>
