    <fields>
        <fullName>Forecast_Month_Date__c</fullName>
        <externalId>false</externalId>
        <formula>DATE(VALUE( ParentCampaign__r.Year__c ),
CASE(Forecast_Month__c, 
&apos;Jan&apos;,1,
&apos;Feb&apos;,2,
&apos;Mar&apos;,3,
&apos;Apr&apos;,4,
&apos;May&apos;,5,
&apos;Jun&apos;,6,
&apos;Jul&apos;,7,
&apos;Aug&apos;,8,
&apos;Sep&apos;,9,
&apos;Oct&apos;,10,
&apos;Nov&apos;,11,
&apos;Dec&apos;,12,1)
,1)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Forecast Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
