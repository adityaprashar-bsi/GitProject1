        <fullName>ForecastCurrencyEqualsCampaign</fullName>
        <active>false</active>
        <description>Forecast Currency must equal Campaign Currency.</description>
        <errorConditionFormula>IF( RecordType.DeveloperName = &quot;Forecast&quot;,  text(CurrencyIsoCode)  &lt;&gt;   text(ParentCampaign__r.CurrencyIsoCode), null)</errorConditionFormula>
