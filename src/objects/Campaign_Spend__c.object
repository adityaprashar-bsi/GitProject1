    <fields>
        <fullName>ReportingDate__c</fullName>
        <description>A common date field that is used to compare Actuals to Committed to Forecast in reports. Actuals mirror Posting Date, Committed mirrors Debit Date, and Forecast mirrors Forecast Month  Date.</description>
        <externalId>false</externalId>
        <formula>IF(RecordType.Name = &quot;Forecast&quot; &amp;&amp; NOT(ISNULL(Forecast_Month_Date__c)), Forecast_Month_Date__c,
  IF(RecordType.Name = &quot;Actual&quot; &amp;&amp; NOT(ISNULL(Posting_Date__c)), Posting_Date__c,
    IF(RecordType.Name = &quot;Committed&quot; &amp;&amp; NOT(ISNULL(Debit_Date__c)), Debit_Date__c,      
      null)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Reporting Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
